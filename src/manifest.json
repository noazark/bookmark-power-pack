{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/noazark/bookmark-power-pack",
  "description": "Access your bookmarks like you've never been able to before.",
  "default_locale": "en",
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "override.html"
  },
  "chrome_settings_overrides" : {
    "homepage": "override.html"
  }
}
